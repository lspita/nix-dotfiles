{
  "restore_on_startup": "none",
  "edit_predictions": {
    "mode": "subtle"
  },
  "format_on_save": "on",
  "features": {
    "edit_prediction_provider": "zed"
  },
  "agent": {
    "dock": "left"
  },
  "auto_signature_help": true,
  "base_keymap": "VSCode",
  "buffer_font_features": {
    "calt": false
  },
  "file_types": {
    "Shell Script": [".envrc"],
    "YAML": [".clangd", ".clang-tidy", ".clang-format"]
  },
  "icon_theme": {
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Latte",
    "mode": "system"
  },
  "inlay_hints": {
    "enabled": true
  },
  "linked_edits": true,
  "middle_click_paste": false,
  "minimap": {
    "show": "always"
  },
  "project_panel": {
    "dock": "right",
    "hide_root": true
  },
  "show_edit_predictions": true,
  "show_signature_help_after_edits": true,
  "show_whitespaces": "selection",
  "tabs": {
    "git_status": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "blinking": "on"
  },
  "theme": {
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Latte",
    "mode": "system"
  },
  "title_bar": {
    "show_branch_icon": true
  },
  "ui_font_features": {
    "calt": false
  }
}
