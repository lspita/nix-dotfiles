{
  "agent": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4-thinking"
    }
  },
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null,
      "enterprise_uri": null
    },
    "enabled_in_text_threads": false
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "auto_install_extensions": {
    "catppuccin": true,
    "catppuccin-icons": true,
    "git-firefly": true,
    "nix": true,
    "dockerfile": true,
    "docker-compose": true,
    "log": true,
    "ssh-config": true,
    "markdown-oxide": true,
    "toml": true
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "scrollbar": {
    "show": "never"
  },
  "minimap": {
    "show": "always"
  },
  "inlay_hints": {
    "enabled": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Latte"
  },
  "icon_theme": {
    "mode": "system",
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Latte"
  },
  "file_types": {
    "Shell Script": [".envrc"]
  }
}
