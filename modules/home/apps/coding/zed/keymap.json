[
  {
    "bindings": {
      "ctrl-;": "terminal_panel::Toggle",
      "ctrl-shift-alt-n": "workspace::NewWindow",
      "ctrl-alt-b": "workspace::ToggleLeftDock",
      "ctrl-b": "workspace::ToggleRightDock",
      "ctrl-r": [
        "projects::OpenRecent",
        {
          "create_new_window": false
        }
      ],
      "ctrl-shift-j": "workspace::ToggleBottomDock",
      "ctrl-j": null,
      "ctrl-shift-n": null
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "ctrl-shift-n": "project_panel::NewDirectory",
      "ctrl-alt-n": null
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-shift-e": "workspace::ToggleEditPrediction",
      "ctrl-alt-shift-e": null
    }
  }
]
